{
  "name": "angular-form-lib",
  "version": "0.2.7",
  "authors": [
    "Brett Uglow <buglow@odecee.com.au>"
  ],
  "description": "Angular Form Library is a form component library that makes it simple to create forms with built-in validation and BootStrap styling.",
  "main": "lib/ng-form-lib.js",
  "moduleType": [
    "globals"
  ],
  "keywords": [
    "angular",
    "forms",
    "bootstrap"
  ],
  "license": "MIT",
  "private": false,
  "ignore": [
    "**/.*",
    "Gruntfile.js",
    "/bower_components",
    "/config",
    "/dev",
    "/dist",
    "/node_modules",
    "/reports",
    "/src/**"
  ],
  "dependencies": {
    "angular": ">=1.2",
    "angular-animate": ">=1.2",
    "angular-scroll": "*",
    "angular-translate": "2.x",
    "angular-translate-loader-static-files": "2.x"
  },
  "devDependencies": {
    "angular-mocks": "1.2.x",
    "angular-strap": "~2.1.4",
    "bootstrap-stylus": "~3.3.1",
    "highlightjs": "~8.3.0"
  }
}
